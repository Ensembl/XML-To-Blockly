<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <start>
    <element name="kitchen">
      <choice>
        <oneOrMore>
          <element name="spoon">
            <text/>
          </element>
        </oneOrMore>
        <oneOrMore>
          <element name="fork">
            <text/>
          </element>
